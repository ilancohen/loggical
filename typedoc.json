{
  "name": "Loggical API Documentation",
  "entryPoints": ["src/index.ts"],
  "exclude": ["**/__tests__/**", "**/*.test.ts", "**/*.spec.ts"],
  "tsconfig": "tsconfig.typedoc.json",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "readme": "none",
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Transports", 
    "Formatters",
    "Configuration",
    "Namespaces",
    "Types",
    "*"
  ],
  "sort": ["source-order"],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Function",
    "Variable"
  ],
  "gitRevision": "main",
  "gitRemote": "origin",
  "hideGenerator": true,
  "hidePageTitle": false,
  "hideBreadcrumbs": false,
  "mergeReadme": false,
  "outputFileStrategy": "members",
  "flattenOutputFiles": true,
  "fileExtension": ".md",
  "entryFileName": "index",
  "indexFormat": "table",
  "parametersFormat": "table",
  "enumMembersFormat": "table",
  "useCodeBlocks": true,
  "expandParameters": true,
  "expandObjects": true,
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": false
  },
  "treatWarningsAsErrors": false,
  "treatValidationWarningsAsErrors": false
}
